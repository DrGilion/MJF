:margharitaPizza a :dish .
:margharitaPizza foaf:name "margharita" .
:margharitaPizza :contains :flour .
:margharitaPizza :contains :egg .
:margharitaPizza :contains :water .
:margharitaPizza :contains :cheese .
:margharitaPizza :contains :tomato .
:margharitaPizza :contains :basil .

:hamburger a :dish .
:hamburger foaf:name "hamburger" .
:hamburger :contains :bread .
:hamburger :contains :tomato .
:hamburger :contains :pickles .
:hamburger :contains :cheese .
:hamburger :contains :onion .
:hamburger :contains :lettuce .
:hamburger :contains :mincedMeat .
:hamburger :contains :ketchup .

:brownie a :dish .
:brownie foaf:name "brownie" .
:brownie :contains :chocolate .
:brownie :contains :flour .
:brownie :contains :egg .
:brownie :contains :sugar .
:brownie :contains :butter .
:brownie :contains :walnut .

#the fridge
:fridge a rdfs:Class .
:hasFood rdfs:domain :fridge .
:hasFood rdfs:range :food .

:fridge :hasFood :lemon .
:fridge :hasFood :bacon .
:fridge :hasFood :milk .
:fridge :hasFood :brownie .
